{
	"name": "@gooi/contracts-capability",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/contracts-capability/contracts-capability.contracts.js",
	"types": "./src/contracts-capability/contracts-capability.contracts.ts",
	"exports": {
		".": {
			"types": "./src/contracts-capability/contracts-capability.contracts.ts",
			"bun": "./src/contracts-capability/contracts-capability.contracts.ts",
			"import": "./dist/contracts-capability/contracts-capability.contracts.js",
			"default": "./dist/contracts-capability/contracts-capability.contracts.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"test": "bun test",
		"build": "bun build ./src/contracts-capability/contracts-capability.contracts.ts --outfile ./dist/contracts-capability/contracts-capability.contracts.js --external zod --external @gooi/stable-json",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"@gooi/stable-json": "workspace:*",
		"zod": "^4.3.6"
	}
}
