{
	"name": "@gooi/entrypoint-runtime",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/execute-entrypoint.js",
	"types": "./src/execute-entrypoint.ts",
	"exports": {
		".": {
			"types": "./src/execute-entrypoint.ts",
			"bun": "./src/execute-entrypoint.ts",
			"import": "./dist/execute-entrypoint.js",
			"default": "./dist/execute-entrypoint.js"
		},
		"./contracts": {
			"types": "./src/contracts.ts",
			"bun": "./src/contracts.ts",
			"import": "./dist/contracts.js",
			"default": "./dist/contracts.js"
		},
		"./domain-runtime-port": {
			"types": "./src/domain-runtime-port.ts",
			"bun": "./src/domain-runtime-port.ts",
			"import": "./dist/domain-runtime-port.js",
			"default": "./dist/domain-runtime-port.js"
		},
		"./idempotency-store": {
			"types": "./src/idempotency-store.ts",
			"bun": "./src/idempotency-store.ts",
			"import": "./dist/idempotency-store.js",
			"default": "./dist/idempotency-store.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"test": "bun test",
		"build": "bun build ./src/*.ts --outdir ./dist --external zod",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"@gooi/contracts-capability": "workspace:*",
		"@gooi/spec-compiler": "workspace:*",
		"@gooi/surface-bindings": "workspace:*",
		"zod": "^4.3.6"
	}
}
