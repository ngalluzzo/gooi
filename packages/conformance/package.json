	"version": "0.1.0",
	"type": "module",
	"main": "./dist/provider-conformance/run-provider-conformance.js",
	"types": "./src/provider-conformance/run-provider-conformance.ts",
	"exports": {
		".": {
			"types": "./src/provider-conformance/run-provider-conformance.ts",
			"bun": "./src/provider-conformance/run-provider-conformance.ts",
			"import": "./dist/provider-conformance/run-provider-conformance.js",
			"default": "./dist/provider-conformance/run-provider-conformance.js"
		},
		"./provider": {
			"types": "./src/provider-conformance/run-provider-conformance.ts",
			"bun": "./src/provider-conformance/run-provider-conformance.ts",
