{
	"name": "@gooi/execution-kernel",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/create-kernel-runtime.js",
	"types": "./src/create-kernel-runtime.ts",
	"exports": {
		".": {
			"types": "./src/create-kernel-runtime.ts",
			"bun": "./src/create-kernel-runtime.ts",
			"import": "./dist/create-kernel-runtime.js",
			"default": "./dist/create-kernel-runtime.js"
		},
		"./create": {
			"types": "./src/create-kernel-runtime.ts",
			"bun": "./src/create-kernel-runtime.ts",
			"import": "./dist/create-kernel-runtime.js",
			"default": "./dist/create-kernel-runtime.js"
		},
		"./invoke": {
			"types": "./src/invoke.ts",
			"bun": "./src/invoke.ts",
			"import": "./dist/invoke.js",
			"default": "./dist/invoke.js"
		},
		"./trace": {
			"types": "./src/trace.ts",
			"bun": "./src/trace.ts",
			"import": "./dist/trace.js",
			"default": "./dist/trace.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"test": "bun test",
		"build": "bun build ./src/create-kernel-runtime.ts --outfile ./dist/create-kernel-runtime.js --external @gooi/kernel-host-bridge/host-portset && bun build ./src/invoke.ts --outfile ./dist/invoke.js && bun build ./src/trace.ts --outfile ./dist/trace.js",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"@gooi/kernel-host-bridge": "workspace:*"
	}
}
