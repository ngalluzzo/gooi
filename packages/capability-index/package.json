{
	"name": "@gooi/capability-index",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/build-capability-index-snapshot.js",
	"types": "./src/build-capability-index-snapshot.ts",
	"exports": {
		".": {
			"types": "./src/build-capability-index-snapshot.ts",
			"bun": "./src/build-capability-index-snapshot.ts",
			"import": "./dist/build-capability-index-snapshot.js",
			"default": "./dist/build-capability-index-snapshot.js"
		},
		"./contracts": {
			"types": "./src/contracts.ts",
			"bun": "./src/contracts.ts",
			"import": "./dist/contracts.js",
			"default": "./dist/contracts.js"
		},
		"./parse": {
			"types": "./src/parse-capability-index-snapshot.ts",
			"bun": "./src/parse-capability-index-snapshot.ts",
			"import": "./dist/parse-capability-index-snapshot.js",
			"default": "./dist/parse-capability-index-snapshot.js"
		},
		"./resolve": {
			"types": "./src/resolve-capability.ts",
			"bun": "./src/resolve-capability.ts",
			"import": "./dist/resolve-capability.js",
			"default": "./dist/resolve-capability.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"test": "bun test",
		"build": "bun build ./src/*.ts ./src/internal/*.ts --outdir ./dist --external zod --external @gooi/contracts-capability",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"@gooi/contracts-capability": "workspace:*",
		"zod": "^4.3.6"
	}
}
