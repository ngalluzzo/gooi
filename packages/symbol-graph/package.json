{
	"name": "@gooi/symbol-graph",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/build-symbol-graph-snapshot.js",
	"types": "./src/build-symbol-graph-snapshot.ts",
	"exports": {
		".": {
			"types": "./src/build-symbol-graph-snapshot.ts",
			"bun": "./src/build-symbol-graph-snapshot.ts",
			"import": "./dist/build-symbol-graph-snapshot.js",
			"default": "./dist/build-symbol-graph-snapshot.js"
		},
		"./contracts": {
			"types": "./src/contracts.ts",
			"bun": "./src/contracts.ts",
			"import": "./dist/contracts.js",
			"default": "./dist/contracts.js"
		},
		"./parse": {
			"types": "./src/parse-symbol-graph-snapshot.ts",
			"bun": "./src/parse-symbol-graph-snapshot.ts",
			"import": "./dist/parse-symbol-graph-snapshot.js",
			"default": "./dist/parse-symbol-graph-snapshot.js"
		},
		"./derive-signal-impact-edges": {
			"types": "./src/derive-signal-impact-edges.ts",
			"bun": "./src/derive-signal-impact-edges.ts",
			"import": "./dist/derive-signal-impact-edges.js",
			"default": "./dist/derive-signal-impact-edges.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p tsconfig.json",
		"test": "bun test",
		"build": "bun build ./src/*.ts ./src/internal/*.ts --outdir ./dist --external zod",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format . --write"
	},
	"dependencies": {
		"zod": "^4.3.6"
	}
}
