		"@gooi/entrypoint-runtime": "workspace:*",
		"@gooi/host-contracts": "workspace:*",
		"@gooi/provider-runtime": "workspace:*",
		"@gooi/product-authoring-lsp": "workspace:*",
		"@gooi/spec-compiler": "workspace:*",
		"@gooi/surface-bindings": "workspace:*",
