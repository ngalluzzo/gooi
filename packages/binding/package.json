		"build": "bun build ./src/binding-plan/binding-plan.ts --outfile ./dist/binding-plan/binding-plan.js --external zod && bun build ./src/binding-plan/contracts.ts --outfile ./dist/binding-plan/contracts.js --external zod && bun build ./src/binding-plan/parse.ts --outfile ./dist/binding-plan/parse.js --external zod && bun build ./src/binding-plan/lookup.ts --outfile ./dist/binding-plan/lookup.js --external zod && bun build ./src/lockfile/contracts.ts --outfile ./dist/lockfile/contracts.js --external zod && bun build ./src/lockfile/parse.ts --outfile ./dist/lockfile/parse.js --external zod && bun build ./src/lockfile/lookup.ts --outfile ./dist/lockfile/lookup.js --external zod && bun build ./src/lockfile/integrity.ts --outfile ./dist/lockfile/integrity.js --external zod && bun build ./src/reachability/contracts.ts --outfile ./dist/reachability/contracts.js --external zod && bun build ./src/reachability/policy.ts --outfile ./dist/reachability/policy.js --external zod && bun build ./src/artifact-alignment/policy.ts --outfile ./dist/artifact-alignment/policy.js --external zod",
